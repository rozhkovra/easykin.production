<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>easykinServlet</servlet-name>
        <servlet-class>ru.rrozhkov.easykin.http.EasyKinServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>easykinServlet</servlet-name>
        <url-pattern>/easykin</url-pattern>
    </servlet-mapping>

  <jsp-config>
  	<jsp-property-group>
    		<url-pattern>*.jsp</url-pattern>
    		<page-encoding>UTF-8</page-encoding>
  	</jsp-property-group>
  </jsp-config>
</web-app>
